{
    "name": "Adversarial Robustness Test",
    "dataset": {
        "name": "smallNORB",
        "input_shape": [96, 96, 1],
        "nb_classes": 5
    },
    "models": {
        "batch": {
            "class": "vgg",
            "name": "vgg_batch",
            "batch_size": 128,
            "group_size": 8,
            "norm_type": "batch",
            "eval": [ "batch" ]
        },
        "local": {
            "class": "vgg",
            "name": "vgg_local",
            "batch_size": 128,
            "group_size": 8,
            "norm_type": "local",
            "eval": [ "single", "single-voting", "voting", "batch" ]
        }
    },
    "train_conf": {
        "learning_rate": 0.0001,
        "epochs": 100,
        "data_split": {
            "train": 8,
            "val": 1,
            "test": 1
        },
        "data_conf": {
        }
    },
    "experiments": {
        "test": {
        },
        "fgm": {
            "adversarial": {
                "attack": "fgm"
            }
        },
        "pgd": {
            "adversarial": {
                "attack": "pgd"
            }
        }
    }
}