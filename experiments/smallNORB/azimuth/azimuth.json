        "batch": {
            "class": "vgg",
            "name": "vgg_batch",
            "batch_size": 128,
            "group_size": 8,
            "norm_type": "batch",
            "eval": [ "batch" ]
        },
        "local": {
            "class": "vgg",
            "name": "vgg_local",
            "batch_size": 128,
            "group_size": 8,
            "norm_type": "local",
            "eval": [ "single", "single-voting", "voting", "batch" ]
